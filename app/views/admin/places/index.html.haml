.row
  .small-10.columns
    %h2 Places
    
    %table.table.table-condensed
      %thead
        %tr
          %th Name
          %th Address
          %th{:colspan => 2}
      %tbody
        - @places.each do |place|
          = render :partial => 'place', :locals => {:place => place}
    %p= link_to 'Enter new place', new_admin_place_path