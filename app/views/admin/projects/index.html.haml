.row
  .small-10.columns
    %h2 Projects
    
    %table.table.table-condensed
      %thead
        %tr
          %th Name
          %th{:colspan => 2}
      %tbody
        - @projects.each do |project|
          = render :partial => 'project', :locals => {:project => project}
    %p= link_to 'Enter new project', new_admin_project_path