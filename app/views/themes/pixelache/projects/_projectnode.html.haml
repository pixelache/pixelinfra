= project.name
- unless project.events.empty?
  %ul.events
    - project.events.sort_by(&:start_at).each do |event|
      %li= event.event_with_date  

- unless project.children.empty?
  %ul
    - project.children.sort_by(&:name).each do |child|
      %li= render :partial => 'projectnode', :locals => {:project => child}
